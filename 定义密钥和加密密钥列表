# 定义密钥和加密密钥列表
key = 'flag'
enkey = [0x40, 0x35, 0x20, 0x56]

# 对加密密钥列表进行异或操作
for i in range(len(enkey)):
    enkey[i] ^= ord(key[i % 4])

# 定义加密数据列表
endate = [0x40, 0x35, 0x20, 0x56, 0x5D, 0x18, 0x22, 0x45, 0x17, 0x2F,
          0x24, 0x6E, 0x62, 0x3C, 0x27, 0x54, 0x48, 0x6C, 0x24, 0x6E,
          0x72, 0x3C, 0x32, 0x45, 0x5B]

# 解密加密数据并打印结果
for i in range(len(endate)):
    print(chr(endate[i] ^ enkey[i % 4]), end='')
